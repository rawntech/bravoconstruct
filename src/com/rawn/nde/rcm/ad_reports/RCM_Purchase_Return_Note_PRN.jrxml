	<parameter name="DocumentNo" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[select o.dateordered as prndate, o.documentno as prnno, bp.name as supplier,
i.documentno as mrrno, i.dateordered as mrrdate,
(select po.documentno from c_order po
where po.c_order_id=i.c_order_id 
and o.documentno='1000000'
and o.ad_client_id='F566B653BBFF417AB00F88941061033E'
) as pono,
 (select po.dateordered from c_order po
where po.c_order_id=i.c_order_id 
and o.documentno='1000000'
and o.ad_client_id='F566B653BBFF417AB00F88941061033E'
) as podate, 
p.value as itemcode, p.name as nameofitem, pc.name as group, u.name as unit, ol.qtyordered as quentity,
ol.priceactual as rate, ol.linenetamt as value
from  c_order o , c_orderline ol, c_bpartner bp, m_inout i, m_inoutline il, m_product p,
c_uom u, m_product_category pc
where o.issotrx='N'
and exists ( select 1 from C_DocType where C_DocType_ID = o.C_DocTypeTarget_ID and isreturn='Y')
and o.documentno='1000000'
and o.c_order_id=ol.c_order_id
and o.c_bpartner_id=bp.c_bpartner_id
and i.m_inout_id=il.m_inout_id
and ol.m_inoutline_id =il.m_inoutline_id
and ol.m_product_id=p.m_product_id
and ol.c_uom_id=u.c_uom_id
and p.m_product_category_id=pc.m_product_category_id
and o.ad_client_id='F566B653BBFF417AB00F88941061033E'
and o.documentno=$P{DocumentNo}]]></queryString>

